&tlmm {
	// 6 10 35~37 39 45~48 55~56 85~86 99 103 118 123 125
	gpio-reserved-ranges = <0 6>, <7 3>, <11 24>, <38 1>, <40 5>, <49 6>, <57 28>, <87 12>, <100 3>, <104 14>, <119 4>, <124 1>, <126 24>;

	volume_gpios: volume-gpios {
		pins = "gpio103", "gpio123";
		function = "gpio";
		drive-strength = <2>;
		bias-pull-up;
	};

	wigig_en_default: wigig-en-state {
		pins = "gpio39";
		function = "gpio";
		output-low;
		drive-strength = <2>;
		bias-disable;
	};

	ts_reset_default: ts-reset-default {
		pins = "gpio99";
		function = "gpio";
		bias-pull-up;
		drive-strength = <8>;
	};

	ts_int_default: ts-int-default {
		pins = "gpio125";
		function = "gpio";
		bias-disable;
		drive-strength = <8>;
		input-enable;
	};

	ts_reset_sleep: ts-reset-sleep {
		pins = "gpio99";
		function = "gpio";
		bias-pull-down;
		drive-strength = <2>;
	};

	ts_int_sleep: ts-int-sleep {
		pins = "gpio125";
		function = "gpio";
		bias-disable;
		drive-strength = <2>;
		input-enable;
	};

	rt1711_int_default: rt1711-int-default {
		pins = "gpio118";
		function = "gpio";
		drive-strength = <2>;
		bias-pull-up;
	};

	pcie0_default_state: pcie0-default {
		clkreq {
			pins = "gpio36";
			function = "pci_e0";
			bias-pull-up;
		};

		reset-n {
			pins = "gpio35";
			function = "gpio";

			drive-strength = <2>;
			output-low;
			bias-pull-down;
		};

		wake-n {
			pins = "gpio37";
			function = "gpio";

			drive-strength = <2>;
			bias-pull-up;
		};
	};

	sde_dsi_active: sde-dsi-active {
		pins = "gpio6";
		function = "gpio";
		drive-strength = <8>;
		bias-disable = <0>;
	};

	sde_dsi_suspend: sde-dsi-suspend {
		pins = "gpio6";
		function = "gpio";
		drive-strength = <2>;
		bias-pull-down;
	};

	mdss_te_default: mdss_te_default {
		pins = "gpio10";
		function = "mdp_vsync";
		drive-strength = <2>;
		bias-pull-down;
	};

	// wcd_intr_default: wcd-intr-default {
	// 	pins = "gpio54";
	// 	function = "gpio";
	// 	input-enable;
	// 	bias-pull-down;
	// 	drive-strength = <2>;
	// };
};

&uart6 {
	status = "okay";

	bluetooth {
		compatible = "qcom,wcn3990-bt";

		/* This path is relative to the qca/ subdir under lib/firmware. */
		firmware-name = "zs600kl/crnv21.bin";

		vddio-supply = <&vreg_s4a_1p8>;
		vddxo-supply = <&vreg_l7a_1p8>;
		vddrf-supply = <&vreg_l17a_1p3>;
		vddch0-supply = <&vreg_l25a_3p3>;
		max-speed = <3200000>;
	};
};

&usb_1 {
	status = "okay";
};

&usb_1_dwc3 {
	dr_mode = "peripheral";
	tx-fifo-resize;
	snps,has-lpm-erratum;
	snps,hird-threshold = /bits/ 8 <0x10>;
	snps,usb3_lpm_capable;
	// workaround for USB timeout issue
	snps,usb2-lpm-disable;
};

&usb_1_hsphy {
	vdda-phy-dpdm-supply = <&vreg_l24a_3p075>;
	vdda-pll-supply = <&vreg_l12a_1p8>;
	vdd-supply = <&vreg_l1a_0p875>;

	qcom,imp-res-offset-value = <0>;
	qcom,bias-ctrl-value = <0x11>;
	qcom,hstx-trim-value = <QUSB2_V2_HSTX_TRIM_24_0_MA>;
	qcom,preemphasis-level = <QUSB2_V2_PREEMPHASIS_15_PERCENT>;
	qcom,preemphasis-width = <QUSB2_V2_PREEMPHASIS_WIDTH_HALF_BIT>;
	qcom,hsdisc-trim-value = <3>;

	status = "okay";
};

&usb_1_qmpphy {
	vdda-pll-supply = <&vreg_l1a_0p875>;
	vdda-phy-supply = <&vreg_l26a_1p2>;
	status = "okay";
};

&usb_2 {
	/* We'll use this as USB 2.0 only */
	qcom,select-utmi-as-pipe-clk;
	status = "okay";
};

&usb_2_dwc3 {
	dr_mode = "otg";
	usb-role-switch;
	role-switch-default-mode = "host";

	tx-fifo-resize;
	snps,has-lpm-erratum;
	snps,hird-threshold = /bits/ 8 <0x10>;
	snps,usb3_lpm_capable;
	// workaround for USB timeout issue
	snps,usb2-lpm-disable;

	/* Fastest mode for USB 2 */
	maximum-speed = "high-speed";

	/* Remove USB3 phy */
	phys = <&usb_2_hsphy>;
	phy-names = "usb2-phy";

	port {
		#address-cells = <1>;
		#size-cells = <0>;

		dwc3_hs: endpoint@0 {
			reg = <0>;
			remote-endpoint = <&usb_con_hs>;
		};

		dwc3_role_switch: endpoint@2 {
			reg = <2>;
			remote-endpoint = <&rt1711h_ep>;
		};
	};
};

&usb_2_hsphy {
	status = "okay";

	vdd-supply = <&vreg_l1a_0p875>;
	vdda-pll-supply = <&vreg_l12a_1p8>;
	vdda-phy-dpdm-supply = <&vreg_l24a_3p075>;

	qcom,imp-res-offset-value = <0>;
	qcom,bias-ctrl-value = <0x10>;
	qcom,hstx-trim-value = <QUSB2_V2_HSTX_TRIM_24_0_MA>;
	qcom,preemphasis-level = <QUSB2_V2_PREEMPHASIS_15_PERCENT>;
	qcom,preemphasis-width = <QUSB2_V2_PREEMPHASIS_WIDTH_HALF_BIT>;
	qcom,hsdisc-trim-value = <3>;
};

&usb_2_qmpphy {
	status = "okay";

	vdda-phy-supply = <&vreg_l26a_1p2>;
	vdda-pll-supply = <&vreg_l1a_0p875>;
};

&ufs_mem_hc {
	reset-gpios = <&tlmm 150 GPIO_ACTIVE_LOW>;
	vcc-supply = <&vreg_l20a_2p95>;
	vccq2-supply = <&vreg_s4a_1p8>;
	vcc-max-microamp = <800000>;
	vccq2-max-microamp = <800000>;
	status = "okay";
};

&ufs_mem_phy {
	vdda-phy-supply = <&vdda_ufs1_core>;
	vdda-pll-supply = <&vdda_ufs1_1p2>;
	status = "okay";
};

&venus {
	firmware-name = "qcom/sdm845/zs600kl/venus.mbn";
	status = "okay";
};

