/dts-v1/;

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/pinctrl/qcom,pmic-gpio.h>
#include <dt-bindings/regulator/qcom,rpmh-regulator.h>
#include <dt-bindings/input/linux-event-codes.h>
#include <dt-bindings/sound/qcom,q6afe.h>
#include <dt-bindings/sound/qcom,q6asm.h>
#include <dt-bindings/usb/pd.h>

/*
 * Asus is using an overclocked Snapdragon 845 SoC on this device, 
 * which has the same frequencies available as the Snapdragon 850
 * used on laptops.
 */
#include "sdm850.dtsi"
#include "pm8998.dtsi"
#include "pmi8998.dtsi"
#include "pm8005.dtsi"

/delete-node/ &rmtfs_mem;

/ {
	model = "Asus ROG Phone";
	compatible = "asus,zs600kl", "qcom,sdm845";
	chassis-type = "handset";

	/* required for bootloader to select correct board */
	qcom,msm-id = <321 0x20000>;
	qcom,board-id = <100 0x0>;

	aliases {
		serial0 = &uart9;
		serial1 = &uart6;
	};

	chosen {
		#address-cells = <2>;
		#size-cells = <2>;
		ranges;

		stdout-path = "display0";

		/* Use framebuffer setup by the bootloader. */
		framebuffer0: framebuffer@9d400000 {
			compatible = "simple-framebuffer";
			reg = <0x0 0x9d400000 0x0 (1080 * 2160 * 4)>;
			width = <1080>;
			height = <2160>;
			stride = <(1080 * 4)>;
			format = "a8r8g8b8";
		};
	};

	gpio-keys {
		compatible = "gpio-keys";

		pinctrl-names = "default";
		pinctrl-0 = <&volume_gpios>;

		key-vol-up {
			autorepeat;
			label = "Volume Up";
			linux,code = <KEY_VOLUMEUP>;
			gpios = <&tlmm 103 GPIO_ACTIVE_LOW>;
			debounce-interval = <15>;
		};

		key-vol-down {
			autorepeat;
			label = "Volume Down";
			linux,code = <KEY_VOLUMEDOWN>;
			gpios = <&tlmm 123 GPIO_ACTIVE_LOW>;
			debounce-interval = <15>;
		};
	};

	reserved-memory {
		/* Framebuffer region */
		memory@9d400000 {
			reg = <0x0 0x9d400000 0x0 0x2400000>;
			no-map;
		};

		asus_debug_mem: removed-region@a0000000 {
			reg = <0x0 0xa0000000 0x0 0x300000>;
			no-map;
		};

		rmtfs_lower_guard: rmtfs-lower-guard@fdf00000 {
			no-map;
			reg = <0x0 0xfdf00000 0x0 0x1000>;
		};

		rmtfs_mem: memory@fdf01000 {
			compatible = "qcom,rmtfs-mem";
			reg = <0x0 0xfdf01000 0x0 0x500000>;
			no-map;

			qcom,client-id = <1>;
			qcom,vmid = <15>;
		};

		rmtfs_upper_guard: rmtfs-upper-guard@fe401000 {
			no-map;
			reg = <0x0 0xfe401000 0x0 0x1000>;
		};
	};

	battery: battery {
		compatible = "simple-battery";

		charge-full-design-microamp-hours = <3800000>;
		voltage-min-design-microvolt = <3400000>;
		voltage-max-design-microvolt = <4358000>;
	};

	btm_vbus_vreg: btm-vbus-vreg {
		compatible = "regulator-fixed";
		regulator-name = "btm_vbus_vreg";

		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;

		gpio = <&pm8998_gpio 6 0>;
		enable-active-high;

		regulator-enable-ramp-delay = <4000>;
	};

	/* pmi8998 otg function is not used */
	pogo_vbus_vreg: pogo-vbus-vreg {
		compatible = "regulator-fixed";
		regulator-name = "pogo_vbus_vreg";

		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;

		gpio = <&pmi8998_gpio 11 0>;
		regulator-always-on;
		regulator-boot-on;
		enable-active-high;

		regulator-enable-ramp-delay = <4000>;
	};

	vreg_s4a_1p8: vreg-s4a-1p8 {
		compatible = "regulator-fixed";
		regulator-name = "vreg_s4a_1p8";

		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		regulator-always-on;
	};

	/* WIGIG_EN pin defined as a fixed regulator */
	wigig_en: wigig-en-1-8v {
		compatible = "regulator-fixed";
		regulator-name = "wigig-en-regulator";
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;

		gpio = <&tlmm 39 GPIO_ACTIVE_HIGH>;
		enable-active-high;
		pinctrl-names = "default";
		pinctrl-0 = <&wigig_en_default>;
	};

	// pmi8998_tcpm: pmic-tcpm {
	// 	compatible = "qcom,pmi8998-tcpm";

	// 	#address-cells = <1>;
	// 	#size-cells = <0>;

	// 	pmic_tcpm_typec = <&pmi8998_typec>;
	// 	pmic_tcpm_pdphy = <&pmi8998_pdphy>;

	// 	connector {
	// 		compatible = "usb-c-connector";

	// 		power-role = "source";
	// 		data-role = "dual";
	// 		self-powered;

	// 		source-pdos = <PDO_FIXED(5000, 3000,
	// 					   PDO_FIXED_DUAL_ROLE |
	// 					   PDO_FIXED_USB_COMM |
	// 					   PDO_FIXED_DATA_SWAP)>;
	// 	};
	// };
};


